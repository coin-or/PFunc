@inproceedings{Veldhuizen:1998:ICASSP,
  author       = "Todd L. Veldhuizen and M. E. Jernigan",
  title	       = "Grid Filters for Local Nonlinear Image Restoration",
  booktitle    = "Proceedings of the International Conference on
