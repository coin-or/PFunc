@InProceedings{siek04:_modular_generics,
  author = 	 {Jeremy Siek and Andrew Lumsdaine},
  title = 	 {Modular Generics},
  booktitle =	 {Concepts: a Linguistic Foundation of Generic Programming},
  year =	 2004,
  month =	 {April},
  organization = {{Adobe Systems}},
  annote =	 {This paper presents a point in the language design space for the support
of generic programming. This point is of interest because it optimizes
modularity and safety, while still affording run-time efficiency.  We
focus on the interface of generic functions: how their type
requirements are described, the name lookup algorithm, and how
separate type checking and separate compilation are achieved.  The
design point presented is fairly close to the type class mechanism of
Haskell, though there are several alterations to improve modularity
and to accommodate common idioms that appear in C++ generic
libraries.
}
}

